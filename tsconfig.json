{
  "$schema": "https://json.schemastore.org/tsconfig",
  "files": [],
  "compilerOptions": {
    "baseUrl": ".",
    "target": "esnext",
    "module": "esnext",
    "sourceMap": false,
    "strict": true,
    "jsx": "preserve",
    "esModuleInterop": true,
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "forceConsistentCasingInFileNames": true,
    "skipLibCheck": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "noImplicitAny": false,
    "noImplicitReturns": true,
    "paths": {
      "@interfaces/*": ["./app/shared/interfaces/*"],
      "@shared/*": ["./app/shared/*"],
      "@modules/*": ["./app/main/modules/*"],
      "@databases/*": ["./app/main/databases/*"],
      "@main-utils/*": ["./app/main/utils/*"],
      "@main/*": ["./app/main/*"],
      "@assets/*": ["./app/renderer/assets/*"],
      "@components/*": ["./app/renderer/components/*"],
      "@features/*": ["./app/renderer/features/*"],
      "@hooks/*": ["./app/renderer/hooks/*"],
      "@stores/*": ["./app/renderer/stores/*"],
      "@render-utils/*": ["./app/renderer/utils/*"],
      "@views/*": ["./app/renderer/views/*"],
      "@renderer/*": ["./app/renderer/*"]
    }
  },
  "references": [{ "path": "./tsconfig.node.json" }, { "path": "./tsconfig.web.json" }]
}
